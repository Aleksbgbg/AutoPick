        <ItemsControl.ItemTemplate>
            <DataTemplate>
                <ContentControl util:View.Attach="{x:Type views:RuneTreeView}" util:View.Model="{Binding}"/>
            </DataTemplate>
        </ItemsControl.ItemTemplate>
        <ItemsControl.ItemsPanel>
            <ItemsPanelTemplate>
                <StackPanel Orientation="Horizontal"/>
            </ItemsPanelTemplate>
        </ItemsControl.ItemsPanel>
    </ItemsControl>
